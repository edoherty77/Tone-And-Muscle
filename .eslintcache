\n  return (\n    <>\n      <IconContext.Provider value={{ color: '#fff' }}>\n        <Nav>\n          <NavbarContainer>\n            <BannerContainer>\n              <BannerPic src=\"images/tma-banner.png\" alt=\"banner\" />\n            </BannerContainer>\n            <MobileIcon onClick={toggle}>\n              <FaBars />\n            </MobileIcon>\n            <NavMenuLeft>\n              <NavItem>\n                <NavLinks\n                  to=\"home\"\n                  smooth={true}\n                  duration={500}\n                  spy={true}\n                  exact=\"true\"\n                  offset={-80}\n                >\n                  Home\n                </NavLinks>\n              </NavItem>\n              <NavItem>\n                <NavLinks\n                  to=\"about\"\n                  smooth={true}\n                  duration={500}\n                  spy={true}\n                  exact=\"true\"\n                  offset={-80}\n                >\n                  About\n                </NavLinks>\n              </NavItem>\n              {/* <NavItem>\n                <NavLinks\n                  to=\"personal\"\n                  smooth={true}\n                  duration={500}\n                  spy={true}\n                  exact=\"true\"\n                  offset={-80}\n                >\n                  Personal\n                </NavLinks>\n              </NavItem> */}\n            </NavMenuLeft>\n            <NavMenuRight>\n              <NavItem>\n                <NavLinks\n                  to=\"testies\"\n                  smooth={true}\n                  duration={500}\n                  spy={true}\n                  exact=\"true\"\n                  offset={-80}\n                >\n                  Success Stories\n                </NavLinks>\n              </NavItem>\n              <NavBtn>\n                <NavBtnLink>Apply Here</NavBtnLink>\n              </NavBtn>\n            </NavMenuRight>\n          </NavbarConta